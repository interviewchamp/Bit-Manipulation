#include<bits/stdc++.h>
using namespace std;


const char n1='\n';
const int mod=1e9+7;


int setbits(int n){

    int ct=0;

    for(int i=0;i<32;i++){

        if((n>>i)&1){
            ct++;
        }
    }
    cout<<ct<<n1;

}
int main(){
     
    ios_base::sync_with_stdio(false);
    cin.tie(0);
   
    
    int n;
    
     
     cin>>n;   //n<=10^9

     setbits(n);

}
